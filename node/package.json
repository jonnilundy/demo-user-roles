{
  "name": "auth-userroles",
  "version": "1.0.0",
  "description": "Display how to manage user roles using WeDeploy",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -w src/index.js -w routes --exec \"babel-node src/index.js --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "babel-node src/index.js --presets es2015,stage-0",
    "prestart": "npm run -s build"
  },
  "eslintConfig": {
    "extends": "liferay",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "handlebars": "^4.0.11",
    "wedeploy": "^4.1.0",
    "wedeploy-middleware": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.9.0",
    "eslint-config-liferay": "^1.0.2",
    "nodemon": "^1.12.1"
  }
}
